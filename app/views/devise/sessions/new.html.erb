<div class="header-wrapper">
 <h2><font color = "white" > Welcome </font></h2>
</div>
<center>
<table border="1" bordercolor="green" width=50% style="margin-top:10%">
 <tr>
    <td>
    <center>
      <h3><font color="green"><u>Login</u></font></h3>  
       <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
         <p> <font color="red">
         <%= flash[:warning] || flash[:notice] || flash[:alert] %>
         </font></p>
         <p>
             <%= f.email_field :email, autofocus: true ,:size=>'18%',:placeholder => 'Enter your e-mail'%>
         </p>
         <p>
            <%= f.password_field :password, autocomplete: "off" ,:size=>'18%',:placeholder => 'Enter your password'%><br>
         
        <% if devise_mapping.rememberable? -%>
        
            <%= f.check_box :remember_me %>
             <%= f.label :remember_me %>
        <% end -%>
        </p>
        <p>
       
            <%= f.submit "Log in" ,:class => "button"%>
       </p>
        <% end %>
        
     <%= render "devise/shared/links" %>
    </center>

    </td>
  </tr>
  </table>

</center>

<div class="footer">jai.rajput@mobiloittegroup.com</div>

