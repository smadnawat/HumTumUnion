<% if @user.errors.any? %>
  <div id="error_explanation">
    <!-- <div>
     The form contains <%#= pluralize(@user.errors.count, "error") %>. 
    </div> -->
    <ul>
    <% @user.errors.full_messages.each do |msg| %>
      <font color = "red"><li><%= msg %></li></font>
    <% end %>
    </ul>
  </div>
<% end %>